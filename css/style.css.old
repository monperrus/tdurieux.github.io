html, body {
    position: relative;
    padding: 0;
    margin: 0;
    background: #E2DEDA;
    width: 100%;
    height: 100%;
    font-family: "Lucida Sans Unicode", "Lucida Grande", sans-serif;
    font-size: 14px;
    color: #424142;
}
ul {
    margin: 0;
    padding: 0;
}
li {
    list-style: none;
}
.clear {
    clear: both;
}

.grid {
    display: block;
    width: 100%;
}

.grid > .col {
    position: relative;
    float: left;

}
.grid > .c1-2 {
    width: 50%;
}

.page {
    position: relative;
    display: block;
    width: 899px;
    min-height: 100%;
    margin: auto;
    background: #EEEAE7;
}
.page > .center, .page > .right {
    min-height: 100%;
    display: inline-block;
}
.page > .center {
    position: relative;
    width: 620px;
    padding: 25px;
    overflow: hidden;
}
.page > .right {
    position: absolute;
    width: 230px;
    background: #FFF;
}

.page > .center > section {
    padding-left: 30px;
}

h1 {
    text-transform: uppercase; 
}
h2 {
    text-transform: uppercase;
}
.firstUpper {
    text-transform: capitalize;

}

.labelData > .label {
    display: inline-block;
}
.labelData > .data {
    display: inline-block;
}

.info {
    margin-bottom: 60px;
}


ul.bull {
    border-left: 1px solid #648FA8;

    margin-left: 6px;
    padding-left: 20px;
}

.bull > li {
    position: relative;
    list-style: none;
    padding-bottom: 15px;
}

.bull > li:before {
    position: absolute;
    content: "â€¢";
    top: 0px;
    left: -32px;
    font-size: 37px;
    line-height: 22px; 
    color: #648FA8;
    background: #EEEAE7;
}

.brand {
    color: #818285;
}

.logo {
    position: relative;
    display: block;
    margin: auto;
    margin-top: 40px;
    margin-bottom: 34px;
    width: 120px;
    height: 120px;
    border-radius: 50%;
    overflow: hidden;
    background: #414042;
}

.work > h3, .education > h3, .project > h3 {
    display: inline-block;
    margin: 0;
    margin-bottom: 10px;
}
.work > h3 .time, .education > h3 .time, .project > h3 .time {
    display: inline-block;
    font-size: 12px;
    color: #648FA8;
}


.skill {
    position: relative;
    display: block;
    margin: auto;
    width: 110px;
    height: 110px;
    border-radius: 50%;
    overflow: hidden;
    border: 5px solid #EEEAE7;
}